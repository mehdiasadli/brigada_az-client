    "@mantine/emotion": "^7.11.2",
    "@mantine/form": "^7.11.2",
    "@mantine/hooks": "^7.11.2",
    "@mantine/modals": "^7.11.2",
    "@tabler/icons-react": "^3.10.0",
    "@tanstack/react-query": "^5.51.1",
    "@tanstack/react-query-devtools": "^5.51.1",
    "@vercel/analytics": "^1.3.1",
    "axios": "^1.7.2",
